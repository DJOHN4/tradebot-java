	public static class OrderState
    {
        public  String PENDING = "PENDING";
        public  String FILLED = "FILLED";
        public  String CANCELLED = "CANCELLED";
    }
    public class OrderAction
    {
        public  String BUY = "buy";
        public  String SELL = "sell";
    }
    public class OrderType
    {
        public  String MARKET = "Market";
        public  String LIMIT = "Limit";
    }
    public class TradeMode
    {
        public  String FIXED = "FIXED";
        public  String VARIABLE = "VARIABLE";
    }
    public class PositionState
    {
        public  String CLOSED = "CLOSED";
    }
  
}
